{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Profiles": {
    "Watch": {
      "commandName": "Executable",
      "executablePath": "dotnet.exe",
      "workingDirectory": "$(ProjectDir)",
      "CommandLineArgs": "Watch run",
      "LaunchBrower": true,
      "LaunchUrl": "https://localhost:5001/"
    }
  }

}
